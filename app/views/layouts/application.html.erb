<!DOCTYPE html>
<html>
<head>
  <title>Job1</title>
  <%#= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.css">
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap-theme.css">
  
  
</head>
<body>

	<!-- Called in Application Controller, need helper method there -->
	<!--  -->

<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">Old Geezer Jobs</a>
    </div>
    <div class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <li class="active"><a href="/ideas">The Plan</a></li>
		<li class="active"><a href="/ideas">The Plan</a></li>
		<li class="active"><a href="http://localhost:3000/users/sign_in">Sign In</a></li>
		<li class="active"><a href="http://localhost:3000/users/sign_up">Sign Up</a></li>
		<li class="active"><%= link_to "Sign out", destroy_user_session_path, :method => :delete %></li>
		
      </ul>

    </div>
	
	
	<p class="navbar-text pull-right">
	<% if user_signed_in? %>
	  Logged in as <strong><%= current_user.email %></strong>.
	  <%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %> |
	  <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %>
	<% else %>
	  <%= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> |
	  <%= link_to "Login", new_user_session_path, :class => 'navbar-link'  %>
	<% end %>
	
	
	
	
  </div>
</nav>




	<span style="font-size: 200%;"><%#= link_to 'all'  %></span>
	<% states.each do |state| %>
	 	<span style="font-size:200%;">
			<!-- Adding Extra Shit To URL -->
			<%= link_to state.abbreviation, state_jobs_url(state, @jobs) %>
		</span>
	<% end %>
	
	<p><%#= link_to 'New Job', new_state_job_path %></p>

<!-- Params Inspect -->
<%= params.inspect %>

	

	
	 <!-- For Devise -->
	 <% if notice %>
	   <p class="alert alert-success"><%= notice %></p>
	 <% end %>
	 <% if alert %>
	   <p class="alert alert-danger"><%= alert %></p>
	 <% end %>

<%= yield %>


<footer>
  <div class="container">
    Rails Girls 2016
  </div>
</footer>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.js"></script>

</body>
</html>
